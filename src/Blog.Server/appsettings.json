{
  "ConnectionStrings": {
    "BlogDbContext": ""
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.File", "Serilog.Expressions" ],
    "Enrich": [ "WithProperty", "FromLogContext" ],
    "Properties": {
      "ApplicationSource": "Web"
    },
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "",
          "formatter": "Serilog.Formatting.Compact.CompactJsonFormatter, Serilog.Formatting.Compact",
          "rollingInterval": "Day"
        }
      }
    ]
  },
  "AllowedHosts": "*",
  "AuthMock": {
    "Enabled": false
  }
}
